{
  "name": "starpets",
  "version": "1.0.0",
  "description": "STARPETS application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "tsc && NODE_ENV=docker sequelize-cli db:migrate --migrations-path dist/migrations",
    "migrate:undo": "tsc && NODE_ENV=docker sequelize-cli db:migrate:undo --migrations-path dist/migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-cron": "3.0.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-cron": "3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "15.1.3",
    "sequelize": "6.37.7",
    "swagger-jsdoc": "6.2.8",
    "umzug": "3.8.2",
    "winston": "^3.11.0",
    "cron": "^3.1.6",
    "@types/cron": "^2.4.0",
    "node-schedule": "^2.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/swagger-jsdoc": "6.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.3.3"
  }
}
